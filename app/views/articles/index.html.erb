<div class="container">

  <% @articles.each do |article| %>
    <div class="panel panel-default">
      <div class="panel-body">
        <%= link_to article.title, article %> - <i> published <%= time_ago_in_words(article.published_date) %> ago.</i>
      </div>
    </div>
  <% end %>
  <<%= will_paginate @articles %>
</div>
