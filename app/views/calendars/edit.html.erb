<p>Edit Page</p>

<%= form_for :event, url: { action: ""} do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />
  <%= f.label :start_time %>:
  <%= f.datetime_select :start_time %><br />
  <%= f.hidden_field :user_id, :value => current_user.id%><br />
  <%= f.label :submit %>:
  <%= f.submit  %>
<% end %>
