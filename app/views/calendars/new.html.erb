<p>Add Event</p>

<%= form_for :calendar, url: { action: "create"} do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />
  <%= f.label :start_time %>:
  <%= f.datetime_select :start_time %><br />
  <%= f.hidden_field :user_id, :value => current_user.id%><br />
  <%= f.label :submit %>:
  <%= f.submit  %>
<% end %>
